# Define your Node class below:
class Node:
  def __init__(self, value, next_node=None):
    self.value = value
    self.next_node = next_node
    #will begin with the first value mentioned in parenthesis
  def get_value(self):
    return self.value
    #will also begin  with first value, but correspond with 2nd
  def get_next_node(self):
    return self.next_node
    #will also begin  with first value, but correspond with 3rd

  def set_next_node(self, next_node):
    self.next_node = next_node
    #will also begin  with first value, but correspond with 3rd, will be creating 

my_node = Node(44)
print(my_node.get_value())
